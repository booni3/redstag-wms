{
    "name": "booni3/redstag-wms",
    "description": "Red Stag WMS PHP Client",
    "keywords": [
        "booni3",
        "redstag-wms"
    ],
    "homepage": "https://github.com/booni3/redstag-wms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adam Lambert",
            "email": "adam@profilestudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Booni3\\RedstagWms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Booni3\\RedstagWms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
